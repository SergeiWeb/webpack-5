	"private": true,
	"engines": {
		"node": ">= 6.0.0",
		"npm": ">= 3.0.0"
	},
	"scripts": {
		"serve": "cross-env NODE_ENV=development webpack serve --color --progress --config config/webpack.dev.js",
		"build": "cross-env NODE_ENV=production webpack --color --progress --config config/webpack.prod.js"
	},
	"browserslist": {
		"production": [
			"> 1%",
			"not dead",
			"last 3 version",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version",
			"last 1 ie version"
		]
	},
